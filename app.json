{
  "expo": {
    "name": "Noted.",
    "slug": "note-taking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFF00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-app.png",
        "backgroundColor": "#FAFF00"
      },
      "package": "com.noted.powerrangers"
    },

    "web": {
      "favicon": "./assets/images/icon-app.png"
    }
  }
}
